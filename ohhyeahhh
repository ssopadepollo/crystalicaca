local work = game.Workspace
local part = Instance.new("Part", work)
part.Name = "CrystalPart"
local Crystalp = work.CrystalPart
Crystalp.Size = Vector3.new(5, 340, 5)
Crystalp.Anchored = true
Crystalp.Position = Vector3.new(-1092, 138, -1198)
Crystalp.Transparency = 0.7
Crystalp.CanCollide = false
Crystalp.Color = Color3.fromRGB(0, 0, 255)
Crystalp.Material = "Neon"

--Crystal
local plrl = game.Players.LocalPlayer
local work = game.Workspace
local Crystalp = work.CrystalPart

Crystalp.Touched:Connect(function()
plrl.Character.HumanoidRootPart.CFrame = CFrame.new(-1092, 350, -1198)
end)
